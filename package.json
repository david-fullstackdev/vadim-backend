    "chokidar": "^1.7.0",
    "cluster": "^0.7.7",
    "cors": "^2.8.4",
    "csvtojson": "^1.1.7",
    "d3": "^4.10.0",
    "dotenv": "^4.0.0",
    "express": "^4.15.4",
    "express-fileupload": "^0.3.0",
    "express-jwt": "^5.3.0",
    "express-ws": "^3.0.0",
    "fs-extra": "^5.0.0",
    "fs-promise": "^2.0.3",
